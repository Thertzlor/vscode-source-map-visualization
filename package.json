{
  "publisher": "colder",
  "name": "source-map-visualization",
  "displayName": "Source Map Visualization",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@7.14.0",
  "description": "A simple visualization of source map data",
  "author": "Colder",
  "license": "MIT",
  "homepage": "https://github.com/cncolder/vscode-source-map-visualization",
  "repository": {
    "type": "git",
    "url": "https://github.com/cncolder/vscode-source-map-visualization"
  },
  "bugs": {
    "url": "https://github.com/cncolder/vscode-source-map-visualization/issues"
  },
  "keywords": [
    "Source Map",
    "Visualization"
  ],
  "categories": [
    "Visualization"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onCommand:sourceMapVisualization.show"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sourceMapVisualization.show",
        "title": "Source Map Visualization"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "sourceMapVisualization.show",
          "group": "z_commands"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.2",
    "@types/node": "^18.11.3",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vsce": "^2.13.0"
  }
}
